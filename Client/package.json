  "packageManager": "yarn@3.1.1",
  "scripts": {
    "update": "yarn upgrade-interactive",
    "build": "babel Src --extensions \".js,.ts\" --out-dir  Dist --copy-files --no-copy-ignored",
    "start": "NODE_ENV=Production isCompiled=true node Dist/Root.js",
    "start:shard": "NODE_ENV=Production isCompiled=true node Dist/Shard.js",
    "start:dev": "NODE_ENV=Development isCompiled=false ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify --files Src/Root.ts ",
    "start:dev:normal": "NODE_ENV=Production isCompiled=false ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify --files Src/Root.ts "
